        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_textview"
        tools:ignore="SpUsage,TextFields,TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck" />

        <TextView
